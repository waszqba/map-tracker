{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "eofline": true,
    "quotemark": [
      true,
      "single"
    ],
    "no-console": false,
    "trailing-comma": [true, {
      "singleline": "never",
      "multiline": "never"
    }],
    "member-access": [
      false
    ],
    "ordered-imports": [
      true
    ],
    "max-line-length": [
      true,
      120
    ],
    "member-ordering": [
      true,
      {
        "order": "instance-sandwich"
      }
    ],
    "curly": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "array-type": [
      false
    ],
    "no-empty-interface": true,
    "no-empty": true,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "object-literal-sort-keys": false,
    "one-line": [
      false
    ],
    "one-variable-per-declaration": [
      false
    ]
  },
  "rulesDirectory": []
}
